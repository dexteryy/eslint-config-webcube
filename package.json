{
  "name": "eslint-config-webcube",
  "version": "0.1.0-beta.8",
  "description": "A sharable ESLint presets which explicitly configures all-available rules (guaranteed by scripts) from ESLint and commonly used plugins.",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/eslint-config-webcube/issues"
  },
  "homepage": "https://github.com/dexteryy/eslint-config-webcube",
  "repository": "https://github.com/dexteryy/eslint-config-webcube",
  "license": "MIT",
  "main": "index.js",
  "files": [
    ".eslintrc.config.json",
    ".eslintrc.prettier.json",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "scripts": {
    "precommit": "npm run check",
    "prepublish": "npm run check",
    "check": "check-dependencies && eslint-find-rules --unused .eslintrc.prettier.json",
    "stat": "eslint-index .eslintrc.prettier.json --format table",
    "update": "rm -rf ./node_modules && npm i",
    "upgrade": "rm -rf ./node_modules package-lock.json && npm i"
  },
  "peerDependencies": {
    "eslint": "^4.7.0"
  },
  "dependencies": {
    "babel-eslint": "^8.0.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-no-use-extend-native": "^0.3.12",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "prettier": "^1.7.0",
    "strip-json-comments": "^2.0.1"
  },
  "devDependencies": {
    "check-dependencies": "^1.1.0",
    "eslint": "^4.7.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-index": "^1.4.0",
    "husky": "^0.14.3"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "webcube",
    "nodecube"
  ]
}
